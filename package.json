{
  "name": "v-skeletor",
  "version": "0.0.4",
  "description": "Vue 2 Skeleton Loading component.",
  "files": [
    "dist",
    "types"
  ],
  "main": "dist/v-skeletor.cjs.js",
  "module": "dist/v-skeletor.esm.js",
  "types": "types/index.d.ts",
  "scripts": {
    "playground": "parcel playground/index.html --out-dir public",
    "build": "rimraf dist && npm run bundle && npm run bundle:min",
    "bundle": "rollup -c build/rollup.config.ts",
    "bundle:min": "rollup -c build/rollup.min.config.ts",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:css",
    "lintfix": "npm run lint:eslint:fix && npm run lint:prettier:fix && npm run lint:css:fix",
    "lint:js": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"{,!(node_modules|dist)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --fix \"{,!(node_modules|dist)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check \"{,!(node_modules|dist)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|dist)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:css": "stylelint \"{,!(node_modules|dist)/**/}*.{css,scss,vue}\" --ignore-path .gitignore",
    "lint:css:fix": "stylelint --fix \"{,!(node_modules|dist)/**/}*.{css,scss,vue}\" --ignore-path .gitignore",
    "lint-staged": "lint-staged",
    "release": "shipjs prepare",
    "release:auto": "shipjs prepare --yes",
    "release:dry": "shipjs prepare --dry-run",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "keywords": [
    "vue",
    "vue js",
    "vue 2",
    "vue 2 skeleton",
    "vue 2 skeleton loading",
    "vue 2 skeleton loader",
    "vue skeleton",
    "vue skeleton loading",
    "vue skeleton loader"
  ],
  "author": {
    "name": "Vinayak Kulkarni",
    "email": "inbox.vinayak@gmail.com",
    "url": "https://vinayakkulkarni.dev"
  },
  "repository": {
    "url": "git+https://github.com/vinayakkulkarni/v-skeletor",
    "type": "git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "private": false,
  "sideEffects": false,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vinayakkulkarni/v-skeletor/issues"
  },
  "homepage": "https://github.com/vinayakkulkarni/v-skeletor#readme",
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@rollup/plugin-alias": "^3.1.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-beep": "^0.2.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "@rollup/plugin-sucrase": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@vinayakkulkarni/prettier-config-vue": "^1.0.0",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/composition-api": "^1.0.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^7.0.1",
    "lint-staged": "^12.0.3",
    "node-sass": "^6.0.1",
    "parcel": "^2.1.1",
    "prettier": "^2.3.2",
    "rollup": "^2.53.3",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "5.1.9",
    "sass": "^1.35.2",
    "shipjs": "^0.24.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "vue": "^2.6.14"
  }
}
