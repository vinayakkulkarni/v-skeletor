{
  "name": "v-skeletor",
  "version": "1.0.0",
  "description": "Vue 2 Skeleton Loading component.",
  "main": "dist/v-skeletor.cjs.js",
  "module": "dist/v-skeletor.esm.js",
  "types": "types/index.d.ts",
  "scripts": {
    "playground": "parcel playground/index.html --out-dir public",
    "build": "rimraf dist && npm run bundle && npm run bundle:min",
    "bundle": "rollup -c build/rollup.config.js",
    "bundle:min": "rollup -c build/rollup.min.config.js",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:css",
    "lintfix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
    "lint:js": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"{,!(node_modules|dist|playground)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --fix \"{,!(node_modules|dist|playground)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check \"{,!(node_modules|dist|playground)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|dist|playground)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:css": "stylelint \"{,!(node_modules|dist|playground)/**/}*.{css,scss,vue}\" --ignore-path .gitignore",
    "lint:css:fix": "stylelint --fix \"{,!(node_modules|dist|playground)/**/}*.{css,scss,vue}\" --ignore-path .gitignore",
    "lint-staged": "lint-staged",
    "release": "shipjs prepare",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "keywords": [
    "vue",
    "vue js",
    "vue 2",
    "vue 2 skeleton",
    "vue 2 skeleton loading",
    "vue 2 skeleton loader",
    "vue skeleton",
    "vue skeleton loading",
    "vue skeleton loader"
  ],
  "author": {
    "name": "Vinayak Kulkarni",
    "email": "inbox.vinayak@gmail.com",
    "url": "https://vinayakkulkarni.dev"
  },
  "repository": {
    "url": "git+https://github.com/vinayakkulkarni/v-skeletor",
    "type": "git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "private": false,
  "sideEffects": false,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vinayakkulkarni/v-skeletor/issues"
  },
  "homepage": "https://github.com/vinayakkulkarni/v-skeletor#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-beep": "^0.2.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "@vinayakkulkarni/prettier-config-vue": "^1.0.0",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/composition-api": "^1.0.0-rc.13",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.13.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "node-sass": "^6.0.1",
    "parcel": "^1.12.4",
    "prettier": "^2.3.2",
    "rollup": "^2.53.1",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "sass": "^1.27.0",
    "shipjs": "^0.23.3",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "tslib": "^2.0.2",
    "typescript": "^4.3.5",
    "vue": "^2.6.14"
  }
}
